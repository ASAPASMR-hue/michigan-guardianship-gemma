# Michigan Guardianship AI Environment Variables
# Copy this file to .env and add your actual API keys

# =============================================================================
# API KEYS - REPLACE WITH YOUR ACTUAL KEYS
# =============================================================================

# Google AI API Keys
GOOGLE_AI_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_google_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# HuggingFace Token
HUGGING_FACE_HUB_TOKEN=your_huggingface_token_here
HUGGINGFACE_TOKEN=your_huggingface_token_here

# Pinecone Configuration
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_HOST=your_pinecone_host_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=michigan-guardianship-v2

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Server Configuration
PORT=5000
HOST=0.0.0.0
FLASK_DEBUG=false

# Model Settings
USE_SMALL_MODEL=true
EMBEDDING_METHOD=cloud
VECTOR_DB=pinecone

# Performance
MAX_WORKERS=4
BATCH_SIZE=100
CHUNK_SIZE=1000
OVERLAP=200

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=detailed

# Timeouts
HF_HUB_DOWNLOAD_TIMEOUT=3600
REQUEST_TIMEOUT=300

# =============================================================================
# JULES ENVIRONMENT SPECIFIC
# =============================================================================

ENVIRONMENT=jules
PLATFORM=ubuntu-linux
PYTHON_VERSION=3.12.11
AUTO_RELOAD=false